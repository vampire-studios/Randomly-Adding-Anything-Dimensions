{
  "schemaVersion": 1,
  "id": "raa_dimensions",
  "version": "${version}",

  "name": "RAA Dimension Addon",
  "description": "This is an example description! Tell everyone what your addon (mod) is about!",
  "authors": [
    "Vampire Studios"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/vampire-studios/Randomly-Adding-Anything-Dimension"
  },

  "license": "CC0-1.0",
  "icon": "assets/raa_dimensions/icon.png",

  "environment": "*",
  "entrypoints": {
    "raa:addon": [
      "io.github.vampirestudios.raa_dimension.RAADimensionAddon"
    ],
    "raa:addon_client": [
      "io.github.vampirestudios.raa_dimension.RAADimensionAddonClient"
    ],
    "modmenu": [
      "io.github.vampirestudios.raa_dimension.config.screen.ModMenuCompat"
    ]
  },
  "mixins": [
    "raa_dimension.mixins.json"
  ],
  "accessWidener": "raa_dimensions.accesswidener",

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "raa_core": "*"
  }
}
